{
    "expo": {
        "name": "Volu",
        "slug": "Volu",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/logo-volu.png",
        "scheme": "volu",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/volu-icon-sin-fondo.png",
            "resizeMode": "contain",
            "backgroundColor": "#004932"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.anonymous.Navigation"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.anonymous.Navigation"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            [
                "expo-image-picker",
                {
                    "photosPermission": "The app accesses your photos to let you share them with your friends."
                }
            ]
        ]
    }
}
